<div class="modal fade" id="examModal" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="examModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-body" style="font-size: 20px;text-align: center;">
        <p><%= messageList.continueEditConfirm %></p>
        <button type="button" class="btn btn-info btn-exam" v-on:click="handlerContinute">Continue</button>
        <button type="button" class="btn btn-info btn-exam" v-on:click="handlerBackToExam">Back to Exam List screen</button>
      </div>
    </div>
  </div>
</div>